<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="skinnerhfortuneteller.FortuneTellerViewController">
   <children>
      <MenuBar prefHeight="25.0" prefWidth="621.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
                  <MenuItem mnemonicParsing="false" text="Open" />
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="About" />
        </menus>
      </MenuBar>
      <ImageView fx:id="fortuneCookieImageView" fitHeight="150.0" fitWidth="200.0" layoutX="219.0" layoutY="164.0" onMouseClicked="#getFortune" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@cookie.jpg" />
         </image></ImageView>
      <Text fx:id="output" layoutX="221.0" layoutY="120.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Click the Cookie for a fortune!" />
   </children>
</AnchorPane>
